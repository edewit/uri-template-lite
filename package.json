{
  "name": "uri-template-lite",
  "version": "0.1.0",
  "stability": 2,
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "URI Template [RFC 6570] implementation in less than 1kb",
  "keywords": [
    "uri",
    "template",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/litejs/uri-template-lite.git"
  },
  "bugs": {
    "url": "https://github.com/litejs/uri-template-lite/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "testman": "*",
    "browser-upgrade-lite": "*"
  },
  "buildman": {
    "uri-template-min.js": "index.js"
  },
  "testling": {
    "files": "tests/run.js",
    "browsers": {
      "ie": [
        6,
        7,
        8,
        9,
        10
      ],
      "ff": [
        3,
        3.6,
        4,
        18,
        19,
        "nightly"
      ],
      "chrome": [
        4,
        10,
        20,
        24,
        25,
        "canary"
      ],
      "safari": [
        5.1,
        6
      ],
      "opera": [
        10,
        11,
        "11.6",
        12,
        "next"
      ],
      "android-browser": [
        4.2
      ],
      "iphone": [
        6
      ],
      "ipad": [
        6
      ]
    }
  }
}

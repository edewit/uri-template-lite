{
  "name": "uri-template-lite",
  "version": "0.1.10",
  "stability": 2,
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "URI Template [RFC 6570] implementation",
  "keywords": [
    "uri",
    "template",
    "rfc6570",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": "git://github.com/litejs/uri-template-lite.git",
  "bugs": {
    "url": "https://github.com/litejs/uri-template-lite/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "testman": "*",
    "browser-upgrade-lite": ">=0.1.7"
  },
  "buildman": {
    "dist/uri-template-min.js": "index.js",
    "dist/uri-expand.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "PARSE"
    },
    "dist/uri-parse.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "EXPAND"
    }
  }
}

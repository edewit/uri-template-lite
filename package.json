{
  "name": "uri-template-lite",
  "version": "0.1.7",
  "stability": 2,
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "URI Template [RFC 6570] implementation in less than 1kb",
  "keywords": [
    "uri",
    "template",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": "git://github.com/litejs/uri-template-lite.git",
  "bugs": {
    "url": "https://github.com/litejs/uri-template-lite/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "testman": "*",
    "browser-upgrade-lite": ">=0.1.7"
  },
  "buildman": {
    "build/uri-template-min.js": "index.js",
    "build/uri-expand.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "PARSE"
    },
    "build/uri-parse.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "EXPAND"
    }
  },
  "testling": {
    "files": "tests/run.js",
    "browsers": [
      "ie/6..10",
      "ff/3",
      "ff/4",
      "ff/5",
      "ff/20",
      "ff/25",
      "chrome/4",
      "chrome/10",
      "chrome/20",
      "chrome/30",
      "chrome/31",
      "opera/10",
      "opera/11",
      "opera/11.6",
      "opera/12",
      "opera/16",
      "safari/4..6",
      "android-browser/4.2",
      "iphone/6",
      "ipad/6"
    ]
  }
}

{
  "Level 1 Examples Longer" :
  {
    "level": 1,
    "variables": {
       "var"   : "value",
       "hello" : "Hello World!"
     },
     "testcases" : [
        ["http://example.com/{var}", "http://example.com/value"],
        ["http://example.com/{hello}", "http://example.com/Hello%20World%21"]
     ]
  },
  "Level 2 Examples Longer" :
  {
    "level": 2,
    "variables": {
       "var"   : "value",
       "hello" : "Hello World!",
       "path"  : "/foo/bar"
     },
     "testcases" : [
        ["http://example.com/{+var}", "http://example.com/value"],
        ["http://example.com/{+hello}", "http://example.com/Hello%20World!"],
        ["http://example.com/{+path}/here", "http://example.com//foo/bar/here"],
        ["http://example.com/here?ref={+path}", "http://example.com/here?ref=/foo/bar"]
     ]
  },
  "Level 3 Examples Longer" :

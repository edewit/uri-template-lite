       "x"     : "1024",
       "y"     : "768"
     },
     "testcases" : [
        ["http://example.com/map?{x,y}", "http://example.com/map?1024,768"],
        ["http://example.com/{x,hello,y}", "http://example.com/1024,Hello%20World%21,768"],
        ["http://example.com/{+x,hello,y}", "http://example.com/1024,Hello%20World!,768"],
        ["http://example.com/{+path,x}/here", "http://example.com//foo/bar,1024/here"],
        ["http://example.com/{#x,hello,y}", "http://example.com/#1024,Hello%20World!,768"],
        ["http://example.com/{#path,x}/here", "http://example.com/#/foo/bar,1024/here"],
        ["http://example.com/X{.var}", "http://example.com/X.value"],
        ["http://example.com/X{.x,y}", "http://example.com/X.1024.768"],
        ["http://example.com/{/var}", "http://example.com//value"],
        ["http://example.com/{/var,x}/here", "http://example.com//value/1024/here"],
        ["http://example.com/{;x,y}", "http://example.com/;x=1024;y=768"],
        ["http://example.com/{;x,y,empty}", "http://example.com/;x=1024;y=768;empty"],
        ["http://example.com/{?x,y}", "http://example.com/?x=1024&y=768"],
        ["http://example.com/{?x,y,empty}", "http://example.com/?x=1024&y=768&empty="],
        ["http://example.com/?fixed=yes{&x}", "http://example.com/?fixed=yes&x=1024"],
        ["http://example.com/{&x,y,empty}", "http://example.com/&x=1024&y=768&empty="]
     ]
  },
